bind('block_dispense',null,null,@e){
    if(@e['item'] == null){
        die()
    }
    cancel()

    @itemType = @e['item']['type']
    switch(@itemType){
        case '310':
        case '311':
        case '312':
        case '313':
        case '298':
        case '290':
        case '300':
        case '301':
        case '306':
        case '307':
        case '308':
        case '309':
        case '314':
        case '315':
        case '316':
        case '317':
        case '302':
        case '303':
        case '304':
        case '305':
        case '443':
        case '442':
        case '397':
            die()
    }

    if(
        @e['location']['world'] == 'survival112' || 
        @e['location']['world'] == 'survival112_nether' || 
        @e['location']['world'] == 'survival112_the_end' ||
        @e['location']['world'] == 'tutorial'
    ){
        cancel(false)
    }
}

